package ve.com.digitel.notificationsender.to.response;

public class NotificationSenderResponse {

	
	/**
	 * 
	 */
	private static final long serialVersionUID = 12L;
	
	
	
	String mensaje;


	
	public NotificationSenderResponse() {

	}

	public NotificationSenderResponse(String mensaje) {
		super();
		this.mensaje = mensaje;
	}



	/**
	 * @return the mensaje
	 */
	public String getMesanje() {
		return mensaje;
	}



	/**
	 * @param mensaje the mensaje to set
	 */
	public void setMesanje(String mensaje) {
		this.mensaje = mensaje;
	}
	
	
}
